{
    "name": "joomla/keychain",
    "type": "joomla-package",
    "description": "Joomla Keychain Package",
    "keywords": ["joomla", "framework", "keychain"],
    "homepage": "https://github.com/joomla-framework/keychain",
    "license": "LGPL-2.1-or-later",
    "require": {
        "php": "^7.2.5",
        "joomla/crypt": "^2.0",
        "joomla/registry": "^1.6|^2.0"
    },
    "require-dev": {
        "joomla/coding-standards": "^3.0@dev",
        "joomla/console": "^2.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "suggest": {
        "joomla/console": "For managing a keychain through a command line interface"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Keychain\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Keychain\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
